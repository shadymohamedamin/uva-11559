#include <iostream>
#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
int main()
{
int t;
cin>>t;
while(t--)
{
    int a,b;
    cin>>a>>b;
    int s,d;
    s=a+b;
    d=a-b;
    if(s<0||d<0||s%2==1||d%2==1) cout<<"impossible"<<endl;
    else cout<<s/2<<" "<<d/2<<endl;
}
    return 0;
}
