#include <iostream>
#include<bits/stdc++.h>
#define f(v) for(auto &i:v)
using namespace std;
int main()
{
 string name;
 while(getline(cin,name))
 {
     int c=0;
     for(int i=0;i<name.size();i++)
     {
        if(isalpha(name[i]))//if(name[i]==' '&&isalpha(name[i+1]))
         c++;
         while(isalpha(name[i])) i++;
     }
     cout<<c<<endl;
 }



    return 0;
}
